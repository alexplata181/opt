<openerp>
    <data>
        <record id="bank_card_fee_form" model="ir.ui.view">
            <field name="name">bank_card_fee_form</field>
            <field name="model">bank.card.fee</field>
            <field name="arch" type="xml">
                <form string="Cuota">
                    <sheet>
                        <group>
                            <h1>
                                <label
                                    for="name"
                                    class="oe_edit_only"
                                    style="font-weight: bold;"
                                />
                                <field name="name"/>
                            </h1>
                        </group>
                        <group>
                            <group>
                                <field name="bank_card_id"/>
                                <field name="fee_quantity"/>
                            </group>
                            <group>
                                <field name="active"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</openerp>
